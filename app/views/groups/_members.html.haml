- unless group.hide_members && (cannot? :update, group)
  .sub-panel#members-panel
    %h4
      = "#{ t :members } (#{ group.memberships_count })"

    %ul#users-list.clearfix
      - group.members.first(15).each do |user|
        = render 'membership', user: user
      = link_to t(:more), group_memberships_path(group), class: 'bottom-panel-link'
